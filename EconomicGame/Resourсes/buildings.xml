<?xml version="1.0" encoding="utf-8" ?>
<buildings>
	<!-- Жилые дома -->
	<houses>
		<house name="House Example 1">
			<!-- Минимальный период строительства -->
            <buildingPeriod>5</buildingPeriod>

            <!-- Начальная стоимость строительства -->
            <cost>10 000 000</cost>

            <!-- Количество квартир -->
	        <countCondo>45</countCondo>

	        <!-- Среднее количество м^2 в квартире -->
	        <averageSquareMetersCondos>23</averageSquareMetersCondos>
		</house>
		<house name="House Example 2">
			<!-- Минимальный период строительства -->
            <buildingPeriod>7</buildingPeriod>

            <!-- Начальная стоимость строительства -->
            <cost>20 000 000</cost>

            <!-- Количество квартир -->
	        <countCondo>150</countCondo>

	        <!-- Среднее количество м^2 в квартире -->
	        <averageSquareMetersCondos>30</averageSquareMetersCondos>
		</house>
		<house name="House Example 3">
			<!-- Минимальный период строительства -->
            <buildingPeriod>10</buildingPeriod>

            <!-- Начальная стоимость строительства -->
            <cost>30 000 000</cost>

            <!-- Количество квартир -->
	        <countCondo>300</countCondo>

	        <!-- Среднее количество м^2 в квартире -->
	        <averageSquareMetersCondos>40</averageSquareMetersCondos>
		</house>
	</houses>
	<!-- Магазины -->
	<markets>
		<market name="Market Example 1">
			<!-- Минимальный период строительства -->
            <buildingPeriod>5</buildingPeriod>

            <!-- Начальная стоимость строительства -->
            <cost>800 000</cost>

            <!--Максимальный объем прибыли магазина в месяц-->
	        <maxProfit>500 000</maxProfit>

	        <!--Процент увеличения стоимости здания после постройки-->
        	<procentCostInc>23</procentCostInc>
		</market>
		<market name="Market Example 2">
			<!-- Минимальный период строительства -->
            <buildingPeriod>7</buildingPeriod>

            <!-- Начальная стоимость строительства -->
            <cost>1 000 000</cost>

            <!--Максимальный объем прибыли магазина в месяц-->
	        <maxProfit>700 000</maxProfit>

	        <!--Процент увеличения стоимости здания после постройки-->
        	<procentCostInc>40</procentCostInc>
		</market>
		<market name="Market Example 3">
			<!-- Минимальный период строительства -->
            <buildingPeriod>3</buildingPeriod>

            <!-- Начальная стоимость строительства -->
            <cost>500 000</cost>

            <!--Максимальный объем прибыли магазина в месяц-->
	        <maxProfit>100 000</maxProfit>

	        <!--Процент увеличения стоимости здания после постройки-->
        	<procentCostInc>10</procentCostInc>
		</market>
	</markets>
</buildings>